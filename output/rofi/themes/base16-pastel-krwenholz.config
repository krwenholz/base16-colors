! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #a8ff60, #a8ff60, #262626

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #a8ff60, #ffffff, #a8ff60, #a8ff60, #eeeeee
rofi.color-active:  #a8ff60, #b5dcff, #a8ff60, #a8ff60, #b5dcff
rofi.color-urgent:  #a8ff60, #ffb6b0, #a8ff60, #a8ff60, #ffb6b0

! Set the desired separator style
rofi.separator-style: solid
